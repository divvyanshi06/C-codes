#include<stdio.h>
int main(){
	int arr[3][3];
	int i,j,sum=0;
	//input elements;
	printf("Enter numbers:\n");
	for(i=0;i<3;i++){
		for(j=0;j<3;j++){
			scanf("%d",&arr[i][j]);
		}
    }
    //print elements
	printf("Matrix is:\n");
	for(i=0;i<3;i++){
		for(j=0;j<3;j++){
			printf("%d ",arr[i][j]);
			sum += arr[i][j];
		}
	printf("\n");
    }
    printf("\n Sum of all elements = %d\n",sum);
	
	return 0;
}
	
